{
  "name": "genenotebook",
  "version": "0.1.15",
  "repository": "https://github.com/genenotebook/genenotebook",
  "description": "A portable website for browsing and querying genome sequences and annotations",
  "license": "AGPL-3.0",
  "scripts": {
    "preinstall": "meteor help || curl https://install.meteor.com/ | sh",
    "preupdate": "meteor update",
    "docs": "documentation build --shallow imports/** -f html -o api-docs",
    "postdocs": "cp api-docs/index.html docs/pages/api-docs.html && cp -r api-docs/assets docs/",
    "bundle": "./bundle.sh",
    "complete_build": "npm install && npm run bundle",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 meteor run",
    "dev_production": "NODE_OPTIONS=--max-old-space-size=8192 meteor run --production",
    "reset": "meteor reset",
    "lint": "eslint imports",
    "postversion": "pushd scripts && npm version $(jq -r .version ../package.json) && popd"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    }
  },
  "eslintConfig": {
    "extends": "@meteorjs/eslint-config-meteor",
    "settings": {
      "import/resolver": {
        "meteor": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "asteroid": "^2.0.3",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "buffer-shims": "^1.0.0",
    "bufferutil": "^4.0.1",
    "child-process": "^1.0.2",
    "color": "^3.1.1",
    "commander": "^2.20.0",
    "core-js": "^3.0.1",
    "core-util-is": "^1.0.2",
    "d3": "^5.9.2",
    "documentation": "^9.3.1",
    "dot-object": "^1.7.1",
    "fibers": "^3.1.1",
    "file-type": "^10.11.0",
    "fs.extra": "^1.3.2",
    "glob": "^7.1.3",
    "html-entities": "^1.2.1",
    "immutability-helper": "^3.0.0",
    "install": "^0.12.2",
    "lodash": "^4.17.11",
    "meteor-babel-helpers": "0.0.3",
    "meteor-job": "^1.5.2",
    "meteor-later": "^1.2.0",
    "meteor-node-stubs": "^0.4.0",
    "object-hash": "^1.3.1",
    "papaparse": "^4.6.3",
    "pkginfo": "^0.4.1",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react": "^16.8.6",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.8.6",
    "react-mounter": "^1.2.0",
    "react-resize-detector": "^4.1.3",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.3",
    "reactstrap": "^7.1.0",
    "readline-promise": "^1.0.4",
    "recompose": "^0.30.0",
    "rus-diff": "^1.1.0",
    "simpl-schema": "^1.5.5",
    "spawn-promise": "^0.1.8",
    "spawn-sync": "^2.0.0",
    "thread-sleep": "^2.2.0",
    "try-thread-sleep": "^2.0.0",
    "utf-8-validate": "^5.0.2",
    "webfontloader": "^1.6.28",
    "ws": "^6.2.1",
    "xml2js-es6-promise": "^1.1.1"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-meteor": "^5.1.0",
    "eslint-plugin-react": "^7.12.4"
  }
}
